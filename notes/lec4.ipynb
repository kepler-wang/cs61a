   ],
   "source": [
    "def fib(n):\n",
    "    pre, cur = 0, 1\n",
    "    k = 1\n",
    "    while k < n:\n",
    "        pre, cur = cur, pre + cur\n",
    "        k = k + 1\n",
    "    return cur\n",
    "\n",
    "fib(20)"
   ]
  },
